{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "img-upload": [
        "dist/img-upload"
      ],
      "img-upload/*": [
        "dist/img-upload/*"
      ],
      "infinite-loading": [
        "dist/infinite-loading"
      ],
      "infinite-loading/*": [
        "dist/infinite-loading/*"
      ],
      "liff-extend": [
        "dist/liff-extend"
      ],
      "liff-extend/*": [
        "dist/liff-extend/*"
      ]
    }
  }
}